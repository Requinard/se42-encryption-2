<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="800.0" prefWidth="800" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UIController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <PasswordField fx:id="password" layoutX="50.0" layoutY="140.0" promptText="Password" />
                <TextArea fx:id="message" layoutX="232.0" layoutY="140.0" prefHeight="200.0" prefWidth="200.0" />
                <Button fx:id="btnEncrypt" layoutX="473.0" layoutY="140.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" text="Encrypt" />
                <Button fx:id="btnDecrypt" layoutX="473.0" layoutY="188.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" text="Decrypt" />
                <Label layoutX="50.0" layoutY="115.0" text="Password:" />
                <Label layoutX="232.0" layoutY="115.0" text="Message:" />
            </children>
        </AnchorPane>
    </children>
</VBox>
